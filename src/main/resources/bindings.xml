<jxb:bindings xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb" version="2.1"
              extensionBindingPrefixes="xjc">
    <jxb:bindings schemaLocation="blurb.xsd">
        <jxb:bindings node="//xs:element[@name='blurb']">
            <jxb:property>
                <jxb:baseType>
                    <xjc:superClass name="com.jolyjonesfamily.blurb.models.BlurbBase" />
                </jxb:baseType>
            </jxb:property>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>